{
  "name": "typescript-helloworld",
  "version": "1.0.0",
  "description": "This is an example project for the &lt;URL> article to set up Visual Studio code with TypeScript.",
  "main": "out/index.js",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "get": "cdktf get",
    "build": "tsc",
    "lint": "npx prettier --write .",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next"
  },
  "engines": {
    "node": ">=14.0"
  },
  "dependencies": {
    "cdktf": "^0.11.2",
    "cdktf-cli": "^0.11.2",
    "constructs": "^10.1.7",
    "eslint-plugin-simple-import-sort": "^10.0.0"
  },
  "precommit": [
    "lint"
  ],
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^28.1.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.8",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
